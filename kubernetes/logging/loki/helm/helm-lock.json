{
  "repo": {
    "name": "grafana",
    "url": "https://grafana.github.io/helm-charts"
  },
  "chart": {
    "name": "loki",
    "gitValuesPath": "https://raw.githubusercontent.com/grafana/loki/refs/tags/{gitTag}/production/helm/loki/values.yaml",
    "gitTagFormat": "helm-loki-{version}",
    "version": "6.51.0"
  },
  "releaseName": "loki",
  "valuesFile": "./values.yaml",
  "extraTemplateArgs": [
    "-n=logging",
    "--kube-version=1.31"
  ]
}
